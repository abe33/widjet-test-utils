{
  "name": "widjet-test-utils",
  "version": "1.9.1",
  "description": "A bunch of test utilities for widjet plugins",
  "main": "lib/index.js",
  "bin": {
    "widjet-test-server": "./server.js"
  },
  "jsnext:main": "src/index.es6",
  "scripts": {
    "test": "nyc mocha --timeout 5000 --recursive test/**/*.es6 test/*.es6",
    "lcov_report": "nyc report --reporter=lcov",
    "coverage": "npm run lcov_report && codecov",
    "babel": "babel src --out-dir . && chmod +x server.js",
    "lint": "eslint src/*.es6 test/*.es6",
    "lint:fix": "eslint --fix src/*.es6 test/*.es6",
    "esdoc": "esdoc -c esdoc.json",
    "prepublishOnly": "npm run babel",
    "start": "npm run babel && node ./server.js test/**/*.es6"
  },
  "keywords": [
    "widgets",
    "widget",
    "web",
    "widjet",
    "test utilities",
    "test helpers"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/abe33/widjet-test-utils.git"
  },
  "bugs": "http://github.com/abe33/widjet-test-utils/issues",
  "commits": "http://github.com/abe33/widjet-test-utils/commits",
  "author": "Cédric Néhémie <cedric.nehemie@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.3",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "babel-eslint": "^10.0.0",
    "codecov": "^1.0.0",
    "esdoc": "^1.1.0",
    "eslint": "^4.12.1",
    "eslint-config": "^0.3.0",
    "eslint-config-fbjs": "^1.1.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-jasmine": "^2.2.0",
    "eslint-plugin-prefer-object-spread": "^1.1.0",
    "eslint-plugin-react": "^5.2.2",
    "expect.js": "^0.3.1",
    "fbjs": "^0.8.6",
    "mocha": "^6.0.2",
    "mocha-jsdom": "^2.0.0",
    "nyc": "^13.3.0",
    "sinon": "^1.17.4"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "glob": "^7.1.0",
    "rollup": "^0.36.1",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^5.0.4",
    "rollup-plugin-includepaths": "^0.1.6",
    "rollup-plugin-node-resolve": "^2.0.0"
  },
  "babel": {
    "sourceMaps": "inline",
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "include": [
      "src/*.es6"
    ],
    "extension": [
      ".es6"
    ],
    "require": [
      "@babel/register"
    ]
  }
}
